{
  "name": "qc-type_of",
  "version": "0.0.0",
  "author": {
    "name": "Danny Hurlburt",
    "url": "https://github.com/dhurlburtusa"
  },
  "description": "A more useful alternative to `typeof <value>`",
  "keywords": [
    "javascript",
    "typeof",
    "typescript"
  ],
  "homepage": "https://github.com/hypersoftllc/qc-type_of#readme",
  "bugs": {
    "url": "https://github.com/hypersoftllc/qc-type_of/issues"
  },
  "license": "ISC",
  "files": [
    "src/index.ts"
  ],
  "main": "src/index.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hypersoftllc/qc-type_of.git"
  },
  "scripts": {
    "build": "tsc",
    "test": "jest --color --coverage",
    "travisci": "npm test"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/dist/",
      "<rootDir>/node_modules/"
    ],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts",
      "tsx"
    ]
  },
  "devDependencies": {
    "@types/jest": "^19.2.2",
    "jest": "^19.0.2",
    "ts-jest": "^19.0.14",
    "typescript": "^2.3.2"
  }
}
